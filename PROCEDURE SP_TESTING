--RECORDAR COLOCAR RESTRICCINES A LA TABLA PUNTAJE_POSTULANTE. ESTO GENERAR√Å QUE EL PROCEDIMIENTO SE CAIGA PROBABLEMENTE, PERO CONTROLAMOS CON EXCEPTIONES

CREATE OR REPLACE PROCEDURE SP_CALL_ASIGNACION_PUNTAJES 
IS BEGIN
    for i in ( select * from POSTULANTE ) LOOP
        SP_ASIGNACION_PUNTAJE((i.run_postulante));
    end LOOP;
END;
EXECUTE SP_TESTING;

SELECT * FROM PUNTAJE_POSTULANTE;
